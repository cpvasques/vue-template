{
  "name": "vue-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:check": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:fix": "eslint . --fix && prettier --write --list-different .",
    "format": "prettier --write src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .husky/pre-commit"
    }
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@internationalized/date": "^3.6.0",
    "@phosphor-icons/vue": "^2.2.1",
    "@tailwindcss/vite": "^4.0.0",
    "@tanstack/vue-query": "^5.59.1",
    "@tanstack/vue-query-devtools": "^5.60.6",
    "@tanstack/vue-table": "^8.20.5",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.1.0",
    "@vueuse/integrations": "^12.0.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "i18next": "^24.0.2",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.447.0",
    "maska": "^3.0.3",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "radix-vue": "^1.9.7",
    "shadcn-nuxt": "^0.10.4",
    "sortablejs": "1",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.2.1",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^4.3.8",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sass-embedded": "^1.82.0",
    "typescript": "~5.7.3",
