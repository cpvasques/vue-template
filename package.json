{
  "name": "vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "lint:format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .husky/pre-commit",
      "pre-push": "sh .husky/pre-push"
    }
  },
  "dependencies": {
    "axios": "^1.2.2",
    "core-js": "^3.27.1",
    "crypto-js": "^4.1.1",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "mathjs": "^11.8.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-the-mask": "^0.11.1",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^4.3.8",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.0",
    "sass": "^1.69.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  }
}
